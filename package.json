  "scripts": {
    "start": "yarn dev",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack serve --mode development --env development --config webpack.config.js",
    "lint:script": "eslint src --ext '.js,.jsx,.ts,.tsx'",
    "lint:fix": "yarn lint:script --fix && yarn lint:styles --fix",
    "lint:types": "tsc --noEmit",
    "lint:styles": "stylelint '**/*.less'",
    "lint": "yarn lint:script && yarn lint:styles && yarn lint:types",
    "prettier": "prettier -c --write '**/*'",
    "reinstall": "yarn upgrade",
    "build:analyzer": "ENABLE_BUNDLE_ANALYZER=1 yarn build"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
